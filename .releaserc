{
  "verifyConditions": [
    "@semantic-release/github"
  ],
  "prepare": [
    {
      "path": "@semantic-release/changelog",
      "changelogFile": ".github/CHANGELOG.md"
    },
    {
      "path": "@semantic-release/git",
      "assets": ".github/CHANGELOG.md"
    }
  ],
  "publish": [
    "@semantic-release/github"
  ]
}